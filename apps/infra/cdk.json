	"watch": {
		"exclude": [
			"README.md",
			"cdk*.json",
			"**/*.d.ts",
			"**/*.js",
			"tsconfig.json",
			"package*.json",
			"yarn.lock",
			"node_modules",
			"test"
		],
		"include": ["**"]
