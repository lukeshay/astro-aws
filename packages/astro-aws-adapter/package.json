	},
	"devDependencies": {
		"@astro-aws/scripts": "workspace:^",
		"@faker-js/faker": "^7.6.0",
		"@types/aws-lambda": "^8.10.109",
		"@types/node": "^18.11.9",
		"aws-lambda": "^1.0.7",
		"eslint": "^8.28.0",
